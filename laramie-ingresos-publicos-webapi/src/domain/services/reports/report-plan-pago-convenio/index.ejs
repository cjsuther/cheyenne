<div class="page">

    <div class="block2">
        <div class="block-row">
            <div class="text-default0 col-4 borde-tabla borde-tabla-top">Cuenta</div>    
            <div class="text-dynamic1 col-8 borde-tabla borde-tabla-top borde-tabla-right"><%= cuenta %></div>
        </div>
        <div class="block-row">
            <div class="text-default0 col-4 borde-tabla">Nomenclatura Catastral</div>
            <div class="text-dynamic1 col-8 borde-tabla borde-tabla-right"><%= nomenclatura %></div>
        </div>
        <div class="block-row">
            <div class="text-default0 col-4 borde-tabla">Nro. Convenio</div>
            <div class="text-dynamic1 col-8 borde-tabla borde-tabla-right"><%= convenio %></div>
        </div>
        <div class="block-row">
            <div class="text-default0 col-4 borde-tabla">Contribuyente y/o Responsable</div>    
            <div class="text-dynamic1 col-8 borde-tabla borde-tabla-right"><%= firmante %></div>
        </div>
        <div class="block-row">
            <div class="text-default0 col-4 borde-tabla">Dirección / Localidad</div>    
            <div class="text-dynamic1 col-8 borde-tabla borde-tabla-right"><%= domicilio %> / <%= localidad %></div>
        </div>
    </div>

    <div class="block2 m-t-20">
        <div class="block-row">
            <div class="text-default0 col-4">Periodos adeudados Años/Cuotas:</div>    
            <div class="text-dynamic1 col-8">

            </div>
        </div>
    </div>

    <div class="block2 m-t-20">
        <div class="block-row">
            <div class="text-default0 col-4 borde-tabla borde-tabla-top">Cantidad de Cuotas</div>    
            <div class="text-dynamic1 col-8 borde-tabla borde-tabla-top borde-tabla-right"><%= cantidadCuotas %></div>
        </div>
        <div class="block-row">
            <div class="text-default0 col-4 borde-tabla">Total Consolidado</div>    
            <div class="text-dynamic1 col-8 borde-tabla borde-tabla-right"><%= totalConsolidado %></div>
        </div>
    </div>

    <div class="block2 m-t-20">
        <div class="block-row">
            <p class="text-convenio"><%= condiciones %></p>
        </div>
    </div>

</div>