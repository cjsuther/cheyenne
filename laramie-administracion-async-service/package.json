  },
  "homepage": "https://github.com/cheyenne-team/major-laramie-administracion-async-service#readme",
  "devDependencies": {
    "@types/node": "^18.11.9",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1",
    "tslint": "^6.1.3",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "dotenv": "^16.0.3",
    "env-cmd": "^10.1.0",
    "express": "^4.18.2",
    "node-cron": "^3.0.2"
