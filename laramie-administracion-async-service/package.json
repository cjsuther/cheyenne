{
  "name": "major-laramie-administracion-async-service",
  "version": "1.0.0",
  "description": "Servicio administrador de procesos asincronicos (proyecto Major Laramie)",
  "main": "index.js",
  "scripts": {
    "start": "tsc && env-cmd -f .env.develope node dist/app.js",
    "start-prod": "tsc && env-cmd -f .env.production node dist/app.js",
    "dev": "env-cmd -f .env.develope nodemon --config nodemon.json src/app.ts",
    "dev:debug": "env-cmd -f .env.develope nodemon --config nodemon.json --inspect src/app.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cheyenne-team/major-laramie-administracion-async-service.git"
  },
